<resources xmlns:tools="http://schemas.android.com/tools">
<!-- Base application theme. -->
<style name="Theme.CinematesMobile" parent="Theme.AppCompat.DayNight.NoActionBar">
    <!-- Primary brand color. -->
    <item name="colorPrimary">@color/purple_500</item>
    <item name="colorPrimaryVariant">#0e1111</item>
    <item name="colorOnPrimary">#000</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">@color/ColorCineOrange</item>
    <item name="colorSecondaryVariant">@color/ColorCineOrange</item>
    <item name="colorOnSecondary">@color/black</item>
    <!-- Status bar color. -->
    <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

</style>
</resources>